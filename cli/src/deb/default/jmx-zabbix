
# project config

USER=www-data
GROUP=www-data

NAME="jmx-zabbix"
DESC="JMX to Zabbix"

LIB_DIR="/usr/share/$NAME/lib"
CONFIG_DIR="/etc/$NAME"
LOG_DIR="/var/log/$NAME"

DAEMON="/usr/bin/java"
DAEMON_OPTIONS="-Dlogback.configurationFile=${CONFIG_DIR}/logback.xml -jar ${LIB_DIR}/jmx-zabbix.jar ${CONFIG_DIR}/jmx-zabbix.yaml"
